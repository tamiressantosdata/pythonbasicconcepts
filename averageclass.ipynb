   "source": [
    "class Averager():\n",
    "    def __init__(self):\n",
    "        self.series=[]\n",
    "    def __call__(self,new_value):\n",
    "        self.series.append(new_value)\n",
    "        total=sum(self.series)\n",
    "        return total/len(self.series)\n"
   ]
  },
  {
